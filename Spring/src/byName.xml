<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean name="address" class="autowiredByName.Address">  
		<property name="city" value="Varanasi" />
		<property name="state" value="Uttar Pradesh" />
	</bean>  
 
 <bean name="employee" class="autowiredByName.Employee" autowire="byName">
	<property name="empName" value="ssp"/>
    </bean>     
</beans>  