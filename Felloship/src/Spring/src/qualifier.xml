<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:annotation-config/>
	<bean id="ex" class="qualifierAnnotation.Exam" />

	<bean id="student1" class="qualifierAnnotation.Student">
		<property name="age" value="11" />
		<property name="name" value="Sushant" />
	</bean>

	<bean id="student2" class="qualifierAnnotation.Student">
		<property name="age" value="22" />
		<property name="name" value="Ank" />
	</bean>
</beans> 